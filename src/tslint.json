{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": {
      "options": [
        200
      ]
    },
    "member-ordering": false,
    "no-bitwise": true,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "trailing-comma": [
      false,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "no-console": false
  },
  "rulesDirectory": []
}
